{
	"name": "fastnet",
	"version": "0.1.8",
	"author": "pranav s",
	"description": "A fast and easy web framework for nodejs",
	"bugs": "https://github.com/prnv404/fastnet/issues",
	"license": "MIT",
	"main": "./build/index.js",
	"types": "./build/index.d.ts",
	"files": [
		"build/**/*"
	],
	"scripts": {
		"prepare": "husky install",
		"format": "prettier --write .",
		"precommit": "lint-staged",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"commit": "cz",
		"start:server": "nodemon --watch 'examble/**/*.ts' --exec 'ts-node' example/server.ts",
		"clean": "del ./build/*",
		"build": "npm run clean && tsc",
		"pub": "git add . && git commit -m \"Updates\" && npm version patch && npm run build && npm publish"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"devDependencies": {
		"@types/jest": "^29.5.10",
		"@types/node": "^20.9.4",
		"@types/parseurl": "^1.3.3",
		"@types/supertest": "^6.0.2",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^7.1.0",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"del-cli": "^5.1.0",
		"eslint": "^8.49.0",
		"husky": "^8.0.0",
		"jest": "^29.7.0",
		"lint-staged": "^15.0.2",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.3",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.3.2"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/prnv404/fastnet"
	},
	"engines": {
		"node": ">= 19.0.0"
	},
	"keywords": [
		"http",
		"node",
		"webserver"
	],
	"dependencies": {
		"parseurl": "^1.3.3"
	}
}