{
	"name": "fastnet",
	"version": "0.0.0",
	"author": "pranav s",
	"description": "A fast and easy web framework for nodejs",
	"main": "index.js",
	"bugs": "https://github.com/prnv404/fastnet/issues",
	"license": "MIT",
	"scripts": {
		"prepare": "husky install",
		"format": "prettier --write .",
		"precommit": "lint-staged",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:preview": "vitepress preview docs",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"commit": "cz",
		"start:server": "nodemon --watch 'examble/**/*.ts' --exec 'ts-node' example/server.ts"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"lint-staged": {
		".lib/**/*.ts": [
			"eslint --fix",
			"git add"
		]
	},
	"devDependencies": {
		"@types/jest": "^29.5.8",
		"@types/node": "^20.9.0",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.11.0",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.49.0",
		"husky": "^8.0.0",
		"jest": "^29.7.0",
		"lint-staged": "^15.0.2",
		"prettier": "^3.0.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2",
		"vitepress": "^1.0.0-rc.25"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/prnv404/fastnet"
	},
	"engines": {
		"node": ">= 19.0.0"
	},
	"keywords": [
		"http",
		"node",
		"webserver"
	]
}
